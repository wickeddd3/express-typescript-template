{
  "name": "express-typescript-template",
  "version": "1.0.0",
  "description": "An Express.js server template with Typescript",
  "main": "index.ts",
  "scripts": {
    "start": "node dist/index.js",
    "dev": "tsc-watch --onSuccess \"node ./dist/index.js\"",
    "build": "tsc",
    "db:migrate": "npx prisma migrate dev --name user-entity --create-only && npx prisma generate",
    "db:push": "npx prisma db push",
    "db:studio": "npx prisma studio"
  },
  "author": "Philip",
  "license": "ISC",
  "dependencies": {
    "@prisma/client": "^5.22.0",
    "compression": "^1.7.5",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "envalid": "^8.0.0",
    "express": "^4.21.1",
    "helmet": "^8.0.0",
    "module-alias": "^2.2.3",
    "morgan": "^1.10.0",
    "prisma": "^5.22.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/morgan": "^1.9.9",
    "@types/node": "^22.9.0",
    "tsc-watch": "^6.2.1",
    "typescript": "^5.6.3"
  },
  "_moduleAliases": {
    "@": "dist",
    "@/constants": "dist/constants",
    "@/controllers": "dist/controllers",
    "@/exceptions": "dist/exceptions",
    "@/interfaces": "dist/interfaces",
    "@/lib": "dist/lib",
    "@/middlewares": "dist/middlewares",
    "@/repositories": "dist/repositories",
    "@/services": "dist/services",
    "@/types": "dist/types",
    "@/utils": "dist/utils"
  },
  "engines": {
    "node": ">=20.15.0"
  }
}
